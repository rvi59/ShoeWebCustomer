    <button class="btn btn-success" onclick="btnRecover()">Submit</button>

    @if (TempData["recovermessage"] != null)
    {

        string mymessage = TempData["recovermessage"].ToString();

        //string[] parts = mymessage.Split('/');
        //string a = parts[0];
        //string b = parts[1];

        <input type="hidden" id="recoverMessageId" value="@mymessage" />
        
    }



