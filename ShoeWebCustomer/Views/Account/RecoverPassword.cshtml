    <div class="form-group">
        <label for="InputPassword">Enter New Password</label>
        <input type="email" class="form-control" id="InputPassword" aria-describedby="emailHelp">
    </div>
    <div class="form-group">
        <label for="InputCPassword">Re Enter Password</label>
        <input type="password" class="form-control" id="InputCPassword">
